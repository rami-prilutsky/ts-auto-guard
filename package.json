{
  "name": "ts-auto-guard",
  "version": "1.0.0-alpha.1",
  "description": "Generate type guard functions from TypeScript interfaces",
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test && npm run format:check && tape -r ts-node/register tests/**/*.ts | tap-diff",
    "build": "run-script-os",
    "build:win32": "tsc",
    "build:linux": "chmod +x lib/cli.js",
    "prepare": "npm run build",
    "format": "tslint --fix --project ./tsconfig.json && prettier --write src/**/*.ts",
    "format:check": "prettier --list-different src/**/*.ts"
  },
  "bin": {
    "ts-auto-guard": "./lib/cli.js"
  },
  "author": "Rhys van der Waerden",
  "license": "MIT",
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^5.0.5",
    "lodash": "^4.17.11",
    "make-error": "^1.3.5",
    "ts-morph": "^2.0.1",
    "tsconfig": "^7.0.0"
  },
  "devDependencies": {
	"@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/lodash": "^4.14.123",
    "@types/node": "^11.13.7",
    "@types/tape": "^4.2.33",
    "@types/uglify-js": "^3.0.4",
    "cross-env": "^5.2.0",
    "prettier": "^1.17.0",
    "run-script-os": "^1.0.5",
    "tap-diff": "^0.1.1",
    "tape": "^4.10.1",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.4",
    "uglify-js": "^3.5.6"
  }
}
